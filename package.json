{
  "name": "der-raum",
  "version": "1.0.0",
  "description": "Landing page for Berlin-based interior design firm Der Raum",
  "author": "Obst Digital",
  "private": true,
  "scripts": {
    "develop": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && cp _redirects dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-runtime": "^6.26.0",
    "css-vars-ponyfill": "^2.3.0",
    "frontmatter-markdown-loader": "^3.1.0",
    "ie11-custom-properties": "^3.1.0",
    "intersection-observer": "^0.11.0",
    "lodash": "^4.17.15",
    "md-yaml-json": "^1.1.1",
    "node-sass": "^4.13.0",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.4.1",
    "nuxt-svg-loader": "^1.2.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.11",
    "vue-markdown": "^2.2.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/google-analytics": "^2.2.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^1.19.1",
    "push-dir": "^0.4.1",
    "vue-svg-inline-loader": "^1.4.4"
  }
}
